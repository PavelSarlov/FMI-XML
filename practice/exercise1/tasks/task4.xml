<?xml version="1.0" encoding="UTF-8"?>
<!--
Коренът ни е таблицата с excel-ските оператори (excelLogicalOperators).
В нея като ред е представен всеки логически оператор (logicalOperator) с условие (condition), самия оператор (operator), примерна формула (formulaExample) и описание (description).
За специалните символи използваме CDATA.
-->
<excelLogicalOperators>
  <logicalOperator>
    <condition>Equal to</condition>
    <operator>=</operator>
    <formulaExample>=A1=B1</formulaExample>
    <description>The forumla returns TRUE if a value in cell A1 is equal to the values in cell B1; FALSE otherwise.</description>
  </logicalOperator>
  <logicalOperator>
    <condition>Not equal to</condition>
    <operator><![CDATA[<>]]></operator>
    <formulaExample><![CDATA[=A1<>B1]]></formulaExample>
    <description>The formula returns TRUE if a value in cell A1 is not equal to the value in cell B1; FALSE otherwise.</description>
  </logicalOperator>
  <logicalOperator>
    <condition>Greate than</condition>
    <operator><![CDATA[>]]></operator>
    <formulaExample><![CDATA[=A1>B1]]></formulaExample>
    <description>The formula returns TRUE if a value in cell A1 is greater than a value in cell B1; FALSE otherwise.</description>
  </logicalOperator>
  <logicalOperator>
    <condition>Less than</condition>
    <operator><![CDATA[<]]></operator>
    <formulaExample><![CDATA[=A1<B1]]></formulaExample>
    <description>The formula returns TRUE if a value in cell A1 is less than in cell B1; FALSE otherwise.</description>
  </logicalOperator>
  <logicalOperator>
    <condition>Greather than or equal to</condition>
    <operator><![CDATA[>=]]></operator>
    <formulaExample><![CDATA[=A1>=B1]]></formulaExample>
    <description>The formula return TRUE if a value in cell A1 is greater than or equal to the values in cell B1; FALSE otherwise.</description>
  </logicalOperator>
  <logicalOperator>
    <condition>Less than or equal to</condition>
    <operator><![CDATA[<=]]></operator>
    <formulaExample><![CDATA[=A1<=B1]]></formulaExample>
    <description>The formula return TRUE if a value in cell A1 is greater than or equal to the values in cell B1; FALSE otherwise.</description>
  </logicalOperator>
</excelLogicalOperators>
