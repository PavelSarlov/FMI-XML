<?xml version="1.0" encoding="UTF-8"?>
<!--
Указваме си пространството от имена на схемата и я зареждаме. 
На корена му слагаме пространството на базовата схема, а не го използваме по подразбиране,
защото то ще бъде присвоено и на неговите поделементи, които всъщност принадлежат на друго пространство.
-->
<xsd1:root xmlns:xsd1="https://schema1.xsd"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="https://schema1.xsd schema1.xsd">
    <leaf>leaf3</leaf>
    <branch>
        <leaf>Leaf1</leaf>
        <leaf>leaf2</leaf>
        <branch></branch>
    </branch>
</xsd1:root>
